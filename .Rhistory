library(bipartite)
m.fw <- read.csv('http://people.fas.harvard.edu/~matthewklau/data/wine_food.csv')
wine.col <- c('lightyellow','yellow','gold','lightblue','pink','red','darkred','black')
food.col <- c('green','orange','tan','brown','white','blue','darkblue','white','red','brown','grey')
selectInput('Food or Wine','',c("All",rownames(m.fw),colnames(m.fw)))
m <- m.fw
if (input$'Food or Wine' == 'All'){
   m <- m.fw
}else if (input$'Food or Wine' %in% colnames(m)){
   m[,colnames(m) == input$'Food or Wine'] <- m.fw[,colnames(m.fw) == input$'Food or Wine']
}else{
   m[rownames(m) == input$'Food or Wine',] <- m.fw[rownames(m.fw) == input$'Food or Wine',]
}
?selectInput
run()
